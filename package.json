{
  "name": "affordable-docs",
  "version": "0.0.1",
  "description": "A story about Project Affordable purpose and what we are aiming at",
  "author": "Markus Gritsch",
  "license": "MIT",
  "keywords": [
    "story",
    "description"
  ],
  "homepage": "https://affordable.io",
