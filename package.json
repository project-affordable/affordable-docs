    "url": "https://github.com/project-affordable/affordable-docs/issues"
  },
  "scripts": {
    "lint": "eslint './docs/**/*' './src/**/*' './pages/**/*'",
    "next:dev": "next",
    "next:build": "next build",
    "next:start": "next start",
    "app:build": "tsc --project tsconfig.json",
    "loader:build": "tsc --project ./webpack/loader/tsconfig.json",
    "dev": "npm run loader:build && npm run app:build && npm run next:dev",
    "start": "npm run loader:build && npm run app:build && npm run next:build && npm run next:start",
    "now-build": "npm run loader:build && npm run app:build && npm run next:build",
    "clean": "rm -rf dist/ .next/",
    "format:mdx": "prettier --parser mdx --print-width 80 --prose-wrap always --write \"docs/src/pages/**/*.mdx\"",
    "sitemap": "node sitemap-generator"
  },
  "main": "index.js",
